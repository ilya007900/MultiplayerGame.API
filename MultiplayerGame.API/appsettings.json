{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "GameOptions": {
    "FieldWidth": 600,
    "FieldHeight": 400,
    "GameUnitWidth": 120,
    "GameUnitHeight": 60
  }
}
